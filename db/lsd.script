CREATE SCHEMA PUBLIC AUTHORIZATION DBA
SET SCHEMA PUBLIC
CREATE USER SA PASSWORD ""
ALTER USER SA SET INITIAL SCHEMA PUBLIC
GRANT DBA TO SA
CREATE TEXT TABLE PUBLIC.STATION (CODE VARCHAR(10),ALTITUDE DOUBLE,LATITUDE DOUBLE,LONGITUDE DOUBLE,STATIONID INTEGER NOT NULL,CONSTRAINT STATION_PKEY PRIMARY KEY(STATIONID),CONSTRAINT STATION_CODE_KEY UNIQUE(CODE))
CREATE TEXT TABLE PUBLIC.OBSERVATION(SAMPLINGTIME TIMESTAMP(0) WITH TIME ZONE NOT NULL,STATIONID INTEGER NOT NULL,WINDSPEED DOUBLE,RELATIVEHUMIDITY DOUBLE,AIRTEMPERATURE DOUBLE,PRECIPITATION DOUBLE,DEWPOINT DOUBLE,WINDDIRECTION DOUBLE,WINDGUST DOUBLE,PRECIPITATIONACCUMULATED DOUBLE,PRESSURE DOUBLE,PRECIPITATIONSMOOTHED DOUBLE,SOILTEMPERATURE DOUBLE,VISIBILITY DOUBLE,PEAKWINDSPEED DOUBLE,WATERTEMPERATURE DOUBLE,PEAKWINDDIRECTION DOUBLE,CONSTRAINT OBSERVATION_PK PRIMARY KEY(SAMPLINGTIME,STATIONID))
SET TIME ZONE INTERVAL '2:00' HOUR TO MINUTE
ALTER SEQUENCE SYSTEM_LOBS.LOB_ID RESTART WITH 1
SET DATABASE DEFAULT INITIAL SCHEMA INFORMATION_SCHEMA
SET TABLE PUBLIC.STATION SOURCE 'station.csv'
SET TABLE PUBLIC.OBSERVATION SOURCE 'obs3h.csv'
